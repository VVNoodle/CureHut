      <div class="fixed-action-btn">
        <a href="#top" class="btn-floating btn-large blue">
          <i class="large material-icons">arrow_upward</i>
        </a>
      </div>

    <!-- END OF MAIN SECTION -->
    </div>
  </div>

  <!-- SIDEBARD -->
  <div class="col s3">
    <div class="fixed z-depth-2 light-blue lighten-3">
      <div class="section text-siderbar" id="dash_sidebar">
        <div class="container">
          Welcome Back,
          <p><strong><% user.username %>!</strong></p>
        </div>
      </div>
      <hr class="no-pad-or-marg">
      <div class="section text-siderbar light-blue lighten-5">
        <div class="container">

          Time Bar<abbr title="Info">(?)</abbr>
          <div class="progress">
            <div class="determinate" style="width: 70%"></div>
          </div>
          <span>63</span> Minutes Remaining

        </div>
      </div>
      <div class="section text-siderbar light-blue lighten-4">
        <div class="container">

          Commends<abbr title="Info">(?)</abbr>
          <p class="p-sidebar">
            <i class="material-icons">mood</i> #&nbsp;&nbsp;&nbsp;&nbsp;
            <i class="material-icons">lightbulb_outline</i> #
          </p>

        </div>
      </div>
      <div class="section text-siderbar light-blue lighten-5">
        <div class="container">

          Streaks<abbr title="Info">(?)</abbr>
          <p class="p-sidebar">
            # Days
          </p>

        </div>
      </div>
      <hr class="no-pad-or-marg">
      <div class="section light-blue lighten-4 no-pad-or-marg">
        <a class="waves-effect waves-teal btn-flat btn-sidebar center-align" href="#">Chat&nbsp;&nbsp;<i class="material-icons">question_answer</i></a><!--href="portal/chat"-->
      </div>
      <div class="section light-blue lighten-5 no-pad-or-marg">
