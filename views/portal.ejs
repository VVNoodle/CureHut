    <div class="col s3 ">
      <div class="fixed">
        <div class="section light-blue lighten-4">
          <div class="container">
            <p>Welcome Back,
              <%= user.username %>!</p>
          </div>
        </div>
        <div class="section light-blue lighten-5">
          <div class="container">
            <p>Time Bar</p>
            <div class="progress">
              <div class="determinate" style="width: 70%"></div>
            </div>
          </div>
        </div>
        <div class="section light-blue lighten-4">
          <div class="container">
            <p>Commends</p>
          </div>
        </div>
        <div class="section light-blue lighten-5">
          <div class="container">
            <p>Streaks</p>
          </div>
        </div>
        <div class="section light-blue lighten-4">
          <div class="container">
            <p>&nbsp;</p>
          </div>
        </div>
        <div class="section light-blue lighten-5">
          <div class="container">
            <a class="waves-effect waves-teal btn-flat" href="portal/chat">Chat</a>
          </div>
        </div>
        <div class="section light-blue lighten-4">
          <div class="container">
            <a class="waves-effect waves-teal btn-flat">Spectate</a>
          </div>
        </div>
      </div>
    </div>
  </div>


