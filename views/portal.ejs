 <title>Dashboard</title>
 <!--Import Google Icon Font-->
 <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
 <!--Import materialize.css-->
 <link type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-alpha.3/css/materialize.min.css"  media="screen,projection"/>

 <!-- TODO: Better way to link stylesheet -->
 <link rel="stylesheet" href="../public/stylesheets/portal.css"> <!--For server set href="/stylesheets/portal.css" -->
 <!--Let browser know website is optimized for mobile-->
 <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>
<body class="scrollspy" id="top">
 <header class="navbar-fixed">
   <nav>
     <div class="nav-wrapper cyan darken-4">
       <div class="container">
         <a href="/" class="brand-logo">Logo</a>
         <a href="#" data-target="mobile" class="sidenav-trigger"><i class="material-icons">menu</i></a>
         <ul class="right hide-on-med-and-down">
           <% if (user) { %>
           <li><a href="#">Home</a></li>
           <li><a href="/dashboard">Dashboard</a></li>
           <li><a href="/auth/logout">Logout</a></li>
           <% } else { %>
           <li><a href="/auth/login">Login</a></li>
           <li><a href="/auth/login">Sign Up</a></li>
           <% } %>
         </ul>
       </div>
     </div>
   </nav>
 </header>

 <ul class="sidenav" id="mobile">
   <% if (user) { %>
   <li><a href="#">Home</a></li>
   <li><a href="/dashboard">Dashboard</a></li>
   <li><a href="/auth/logout">Logout</a></li>
   <% } else { %>
   <li><a href="/auth/login">Login</a></li>
   <li><a href="/auth/login">Sign Up</a></li>
   <% } %>
 </ul>


<div class="row">
  <div class="col s9">
    <div class="container">
      <h4>Dashboard</h4>
      <div class="divider"></div>

      <!-- MAIN SECTION (JOURNAL ENTRY and JOURNAL HISTORY)-->
      <div class="section">
        <h5>Journal Entry</h5>
        <div class="row">
          <form class="col s12">
            <div class="row">
              <div class="input-field col s6">
                <input id="input_text" type="text" data-length="10">
                <label for="input_text">Entry Title</label>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <textarea id="textarea1" class="materialize-textarea" data-length="120"></textarea>
                <label for="textarea1">Entry</label>
              </div>
            </div>
            <div class="row">
              <div class="col s5">
                <ul class="pagination">
                  <li class="waves-effect"><a href="#!"> <i class="material-icons">mood</i></a></li>
                  <li class="waves-effect"><a href="#!"> <i class="material-icons">mood</i></a></li>
                  <li class="waves-effect"><a href="#!"> <i class="material-icons">mood</i></a></li>
                  <li class="waves-effect"><a href="#!"> <i class="material-icons">mood</i></a></li>
                  <li class="waves-effect"><a href="#!"> <i class="material-icons">mood</i></a></li>
                </ul>
              </div>
              <div class="col s7">
                <input class="btn btn-large" type="submit" name="submit" value="Submit">
              </div>
            </div>
          </form>
        </div>
      </div>

